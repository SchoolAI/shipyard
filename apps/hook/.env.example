# Hook Environment Variables
# Copy this file to .env and adjust values for local development

# Logging
LOG_LEVEL=info

# Registry configuration
REGISTRY_PORT=32191

# State directory (optional - defaults to ~/.peer-plan/)
PEER_PLAN_STATE_DIR=~/.peer-plan

# Web UI URL (for links generated by hook)
PEER_PLAN_WEB_URL=http://localhost:5173
